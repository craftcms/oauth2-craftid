{
  "name": "league/oauth2-craft",
  "description": "Craft OAuth 2.0 Client Provider for The PHP League OAuth2-Client",
  "version": "1.0.0",
  "license": "MIT",
  "authors": [
    {
      "name": "Pixel & Tonic",
      "homepage": "https://pixelandtonic.com/"
    }
  ],
  "keywords": [
    "oauth",
    "oauth2",
    "client",
    "authorization",
    "authentication",
    "cms",
    "craftcms"
  ],
  "minimum-stability": "beta",
  "require": {
    "league/oauth2-client": "^2.0",
    "eloquent/phony": "^0.14.6"
  },
  "require-dev": {
    "phpunit/phpunit": "^5.0",
    "satooshi/php-coveralls": "^1.0",
    "squizlabs/php_codesniffer": "^2.0"
  },
  "autoload": {
    "psr-4": {
      "League\\OAuth2\\Client\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "League\\OAuth2\\Client\\Test\\": "tests/src/"
    }
  }
}
